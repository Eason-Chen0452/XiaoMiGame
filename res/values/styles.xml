    <style name="AppTheme" parent="@style/AppBaseTheme" />
    <style name="AppThemeNoTitle" parent="@android:style/Theme">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="checkBoxStyle">
        <item name="android:button">@drawable/selector_float_tip_check</item>
    </style>
    <style name="splashStyle" parent="@android:style/Theme.Light.NoTitleBar.Fullscreen" />
